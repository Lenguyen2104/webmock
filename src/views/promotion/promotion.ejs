    <script>
      function formatM(params) {
        if (isNaN(params) || params === null || params === undefined || params === 0) {
        return 0; // Nếu đúng, trả về 0
      }
        var money = params;
        var config = {
          style: "currency",
          currency: "VND",
          maximumFractionDigits: 9,
        };
        var formated = new Intl.NumberFormat("vi-VN", config).format(money);
        return formated;
      }

      function RosesRender(datas) {
        let html = datas.map((e) => {
          let html = "";
          return (html += `<div data-v-7c8bbbf4="" class="bd van-row">
                        <div data-v-7c8bbbf4="" class="c-tc van-ellipsis van-col van-col--6">Cấp ${e.level}</div>
