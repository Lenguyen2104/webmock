          $("#first_deposit_users_direct_subordinates").text(response.invite.data_direct_subordinates.first_deposit_users_direct_subordinates);

          $("#code").text(response.info[0].code);
          $("#clipboard-text").text(response.info[0].code);

          $("#clipboard-text").prop("data-clipboard-text", response.info[0].code);
          $("#clipboard-element").attr("data-clipboard-text", response.info[0].code);
          $("#clipboard-element").click(function() {
              var clipboardText = $(this).attr("data-clipboard-text");
              navigator.clipboard.writeText(clipboardText).then(function() {
