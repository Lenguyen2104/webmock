              }).catch(function(err) {
                  console.error('Lỗi khi sao chép:', err);
              });
          });

          $("#napdau").text(response.info[0].napdau);
          $("#napdau1").text(response.info[0].napdau);
          $("#money").text(formatM(response.info[0].money));

